<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.bmi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bmi">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity"/>
        <activity
            android:name=".SubMenuActivity"
            android:label="BMI"
            android:theme="@style/Theme.Bmi.NoActionBar">

        </activity>
        <activity android:name=".BmiActivity"/>
        <activity android:name=".CalculationActivity"/>
        <activity android:name=".BmiResult"/>
        <activity android:name=".SubActivity"
            android:label="다음 페이지"/>
    </application>

</manifest>